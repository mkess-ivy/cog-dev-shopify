<div id="content" class="row">

    <div class="cog-product-wrapper">
        <div class="cog-collection-3col">
            
            {% for link in linklists.artist-collection.links %}
            <div class="collection-thumb-title">
                {{ link.object.title | escape }} 
            </div>
            {% endfor %}

            <div class="frow justify-between">
                {% for product in collections.artist-collection.products limit:4 %}
                    <div class="">{{ collection.title }}</div>
                    <div class="cog-coll-product">
                        <img src="{{ product.featured_image | img_url: 'master' }}" /><br/>
                        {{ product.title }}<br/>
                        {{ product.price  | money_without_trailing_zeros }}
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>

</div>